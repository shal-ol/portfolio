.persent{
    display:inline-block;
    transform: rotate(180deg);
    width: 10.5rem;
    height: 10.5rem;
        @include tablets() {
        width: 9.5rem;
        height: 9.5rem;
        }
&__circle{
    width: 9.5rem;
    height: 9.5rem;
    fill: transparent;
    stroke: #413d3b;
    stroke-width: 2px;
}
&__circle_above{
    stroke-width: 5px;
    stroke: #ee8200;
    stroke-dasharray: 251;

}
}



