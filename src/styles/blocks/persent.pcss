.persent{
    display:inline-block;
    transform: rotate(180deg);
    width: 10.5rem;
    height: 10.5rem;
&__circle{
    fill: transparent;
    stroke: #413d3b;
    stroke-width: 2px;
}
&__circle_above{
    stroke-width: 5px;
    stroke: #ee8200;
    stroke-dasharray: 251;
    stroke-dashoffset: 251;
    animation: fill 100s linear infinite;
    animation-play-state: paused;
}
}
@keyframes fill{
    from{
        stroke-dashoffset: 251;
    }
    to{
        stroke-dashoffset: 0;
    }
}


